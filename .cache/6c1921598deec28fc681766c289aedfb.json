ne":44,"column":21}},{"generated":{"line":73,"column":26},"source":"src\\ItemListFeatureLayer.js","original":{"line":44,"column":4}},{"generated":{"line":74,"column":0},"source":"src\\ItemListFeatureLayer.js","original":{"line":45,"column":4}},{"name":"fLayer","generated":{"line":74,"column":11},"source":"src\\ItemListFeatureLayer.js","original":{"line":45,"column":9}},{"generated":{"line":74,"column":17},"source":"src\\ItemListFeatureLayer.js","original":{"line":45,"column":4}},{"name":"addTo","generated":{"line":74,"column":18},"source":"src\\ItemListFeatureLayer.js","original":{"line":45,"column":16}},{"generated":{"line":74,"column":23},"source":"src\\ItemListFeatureLayer.js","original":{"line":45,"column":4}},{"generated":{"line":74,"column":24},"source":"src\\ItemListFeatureLayer.js","original":{"line":45,"column":22}},{"name":"map","generated":{"line":74,"column":29},"source":"src\\ItemListFeatureLayer.js","original":{"line":45,"column":27}},{"generated":{"line":74,"column":32},"source":"src\\ItemListFeatureLayer.js","original":{"line":45,"column":4}},{"generated":{"line":75,"column":0},"source":"src\\ItemListFeatureLayer.js","original":{"line":46,"column":3}},{"name":"ItemListFeatureLayer","generated":{"line":81,"column":18},"source":"src\\ItemListFeatureLayer.js","original":{"line":49,"column":15}}],"sources":{"src\\ItemListFeatureLayer.js":"import { featureLayer } from \"esri-leaflet\";\nimport L from \"leaflet\";\n\nclass ItemListFeatureLayer {\n  constructor(map, onPopUp, geoRestURL, gisField, styleFnc = null) {\n    if (!map) throw \"map object is requried\";\n    if (!onPopUp) throw \"onPopUp call back required\";\n    if (!geoRestURL) throw \"Rest URL for gis Required\";\n    this.map = map;\n    this.onPopUp = onPopUp;\n    this.gisField = gisField;\n    this.fLayer = featureLayer({\n      url: geoRestURL,\n      fields: [\"*\"],\n      pointToLayer: function(geojson, latlng) {\n        return L.circleMarker(latlng, {\n          color: \"white\",\n          weight: 1,\n          fillColor: \"darkorange\",\n          fillOpacity: 0.6\n        });\n      }\n    });\n  }\n\n  init() {\n    this.fLayer.bindPopup(e => \"...loading\");\n    this.fLayer.once(\"load\", () => {\n      const Bounds = L.latLngBounds([]);\n      this.fLayer.eachFeature(layer =>\n        Bounds.extend(layer._bounds || layer._latlng)\n      );\n    });\n    this.fLayer.on(\"click\", e => this.handleLayerClick(e));\n  }\n\n  handleLayerClick(e) {\n    let gisFieldValue = e.layer.feature.properties[this.gisField];\n    this.onPopUp(gisFieldValue, this.fLayer.getPopup());\n  }\n\n  render() {\n    console.log(\"add to map \");\n    console.log(this.map);\n    this.fLayer.addTo(this.map);\n  }\n}\n\nexport default ItemListFeatureLayer;\n"},"lineCount":null}},"hash":"892a2b40ffad338327a549e14ed2d3f7","cacheData":{"env":{}}}